{
    "family": "structure_frontend",
    "containerDefinitions": [
        {
            "name": "structure_frontend",
            "image": "",
            "cpu": 10,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "geeks-academy",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "structure_frontend"
                }
            },
            "secrets": [
                {
                    "name": "REACT_APP_URL_API",
                    "valueFrom": "/geeksacademy/structure_frontend/REACT_APP_URL_API"
                }
            ]
        }
    ]
}

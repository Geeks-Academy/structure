{
    "family": "structure_backend",
    "executionRoleArn": "",
    "containerDefinitions": [
        {
            "name": "structure_backend",
            "image": "",
            "cpu": 10,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 4000,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "MONGO_CONNECTION_STRING",
                    "valueFrom": "/geeksacademy/mongodb/MONGO_CONNECTION_STRING"
                }
            ]
        }
    ]
}
